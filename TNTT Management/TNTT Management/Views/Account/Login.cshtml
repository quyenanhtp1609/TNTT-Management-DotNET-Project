          href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Material+Icons" />
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <style>

        .footer-form {
            padding: 0 20px;
        }

        .pull-left label,
        .pull-right a,
        .btn {
            font-weight: bold;
        }

        .pull-right a {
            margin-right: 17px;
        }

        .pull-left label {
            margin-top: -2px;
        }
    </style>
</head>
<body class="off-canvas-sidebar">
    <nav class="navbar navbar-primary navbar-transparent navbar-absolute">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navigation-example-2">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <h4>Hệ thống Quản lý Thiếu nhi</h4>
            </div>
        </div>
    </nav>
    <div class="wrapper wrapper-full-page">
        <div class="full-page login-page">
            <!--   you can change the color of the filter page using: data-color="blue | purple | green | orange | red | rose " -->
            <div class="content">
                <div class="container">
                    <div class="row">
                        <div class="col-md-4 col-sm-6 col-md-offset-4 col-sm-offset-3">
                            <form method="post" action="/Account/Login" name="myForm" id="LoginValidation">
                                <div class="card card-login card-hidden">
                                    <div class="card-image" data-header-animation="true">
                                        <img class="img" src="~/Content/assets/img/TNTT.png">
                                    </div>
                                    <div class="card-content">
                                        <div class="input-group">
                                            <span class="input-group-addon">
                                                <i class="material-icons">account_circle_outline</i>
                                            </span>
                                            <div class="form-group label-floating">
                                                <label class="control-label">Tên đăng nhập</label>
                                                <input type="text" class="form-control nospace" data-toggle="tooltip"
                                                       title="Username chưa được nhập" data-placement="left" id="username"
                                                       name="username" required="true" pattern="\S+">
                                            </div>
                                        </div>
                                        <div class="input-group">
                                            <span class="input-group-addon">
                                                <i class="material-icons">lock_outline</i>
                                            </span>
                                            <div class="form-group label-floating">
                                                <label class="control-label">Mật khẩu</label>
                                                <input type="password" class="form-control nospace" id="password"
                                                       name="password" data-toggle="tooltip"
                                                       title="Password chưa được nhập" data-placement="left"
                                                       required="true" pattern="\S+">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="footer-form">
                                        <div class="checkbox pull-left">
                                            <label>
                                                <input type="checkbox" name="optionsCheckboxes">Giữ đăng
                                                nhập
                                            </label>
                                        </div>
                                        <div class="pull-right">
                                            <a href="forgot_password.html">
                                                Quên mật
                                                khẩu?
                                            </a>
                                        </div>
                                    </div>
                                    <div class="footer text-center">

                                            <button type="submit" class="btn btn-primary btn-simple btn-wd btn-lg"
                                                    >
                                                Đăng nhập
                                            </button>
  
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>

                </div>
            </div>
            <footer class="footer">
                <div class="container">
                    <p class="copyright pull-left">
                        &copy;
                        <script>document.write(new Date().getFullYear())</script>
                        TNTT Vietnam | Phát triển bởi QA System
                    </p>
                </div>
            </footer>
        </div>
    </div>
</body>
